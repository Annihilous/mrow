<p style="color: green"><%= notice %></p>

<% content_for :title, "Worksheets" %>

<h1>Worksheets</h1>

<div id="worksheets">
  <% @worksheets.each do |worksheet| %>
    <%= render worksheet %>
    <p>
      <%= link_to "Show this worksheet", worksheet %>
    </p>
  <% end %>
</div>

<%= link_to "New worksheet", new_worksheet_path %>
